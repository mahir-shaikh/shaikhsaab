/* customize scrollbar */
::-webkit-scrollbar {
  width: 5px;
  height: 5px;
}
/* Track */
::-webkit-scrollbar-track {
  -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
  -webkit-border-radius: 10px;
  border-radius: 0px;
}
/* Handle */
::-webkit-scrollbar-thumb {
  border-radius: 5px;
  background: #6d747e;
}
::-webkit-scrollbar-thumb:window-inactive {
  background: #6d747e;
}
html,
body {
  min-height: 100%;
}
body {
